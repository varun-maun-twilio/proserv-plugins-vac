{
  "custom_data": {
    "serverless_functions_domain": "<YOUR_SERVERLESS_DOMAIN>",
    "common": {
      "log_level": "info",
      "audit_log_ttl": 1209600
    },
    "features": {
      "attribute_viewer": {
        "enabled": true,
        "enabled_for_agents": false
      },
      "admin_ui": {
        "enabled": true,
        "enable_audit_logging": true
      },
      "queues_stats_metrics": {
        "enabled": false,
        "assigned_tasks_column": true,
        "wrapping_tasks_column": true,
        "agent_activity_stats_column": true
      },
      "queue_stats_tasklist": {
        "enabled": false
      },
      "queue_stats_tabs": {
        "enabled": false
      },
      "signature_management": {
        "enabled": false
      },
      "outbound_recording": {
        "enabled": true,
        "include_queues": []
      },
      "email_search": {
        "enabled": false
      },
      "worker_capacity": {
        "enabled": false
      },
      "persistent_drafts": {
        "enabled": false
      },
      "bulk_skills_mapper": {
        "enabled": false
      }
    }
  }
}